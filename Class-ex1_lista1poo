        internal class Multi
        {
            #region Atributos
            private int a;
            private int resultado;
            #endregion

            #region MetodosInterface
            public void setA(int p)  
            {
                a = p;
            }
            public int getA()
            {
                return a;
            }
            public int getresultado()
            {
                return resultado;
            }
            #endregion
            #region MÃ©todo funcional
            public void calcular()
            {
                resultado = a * a;
            }
            #endregion
        }
